<html 
xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">

 
<body>
<table  border="1" cellspacing="0" cellpadding="0">
	<tr>
		<td align="center">
			<h1><b>${CompDesc}</b></h1>			
		</td>
	</tr>
	<tr>
		<td align="center">
			<b>ORDER FORM</b>			
		</td>
	</tr>
	<tr>
		<td >
			<table width="100%" border="1" cellspacing="0" cellpadding="0">
				<tr>
					<td>
						<b>Order No:</b>
					</td>
					<td>
						 ${printOrderNo}
					</td>
					<td>
						<b>Order Date: </b>
					</td>
					<td>
						 ${printOrderDate}
					</td>
				</tr>
				
				<tr>
					<td>
						<b>P/O No : </b>
					</td>
					<td>
						 ${printPOno}
					</td>
					<td>
						<b>Delivery Date:</b>
					</td>
					<td>
						 ${printDeliveryDate}
					</td>
				</tr>
				
				<tr>
					<td>
						<b>Customer No:</b>
					</td>
					<td>
						 ${printCustomerNo}
					</td>
					<td >
						<b>Master Code: </b>						
					</td>
					<td>
						${printMasterCard}
					</td>
				</tr>
				
				<tr>
					<td>
						<b>Brand Code:</b>
					</td>
					<td>
						  ${printBrandCode}
					</td>
					<td>
						<b>Process</b> 
					</td>
					<td>
						. 
					</td>
				</tr>
				<tr>
					<td colspan="4"></td>
				</tr>
			</table>	
		</td>
	</tr>
	<tr>
		<td>
			<b>Raw Material:-</b>			
		</td>
	</tr>
	
	<tr>
		<td>
			<table width="100%" border="1" cellspacing="0" cellpadding="0">
				<tr>
					<th width="100"> Code No </th>
					<th width="100"> Film Type </th>
					<th width="100"> Thickness </th>
					<th width="100"> Width </th>
					<th width="100"> Length </th>
					<th width="100"> Quantity </th>
					<th width="100"> Process </th>
					<th width="150"> Production Date </th>					
				</tr>
				<t:loop source="RawMats" value="RawMat">
					<tr>
						<td>${CodeNo}</td>
						<td>${Type}</td>
						<td>${Thickness}</td>
						<td>${Width}</td>
						<td>${Length}</td>
						<td>${Quantity}</td>
						<td>${Process}</td>
						<td>${ProductionDate}</td>
						
						
					</tr>
				</t:loop>

			</table>			
		</td>
	</tr>
	<tr>
		<td height="40">
					
		</td>
	</tr>
	<tr>
		<td >
			<table border="1" width="100%"  cellspacing="0" cellpadding="0">
				<tr>
					<td rowspan="4" width="30%" valign="top" align="right">Production Remark:</td>
					<td width="70%">____________________________________________________________________________________</td>
				</tr>
				<tr>					
					<td>____________________________________________________________________________________</td>
				</tr>
				<tr>					
					<td>____________________________________________________________________________________</td>
				</tr>
				<tr>					
					<td>____________________________________________________________________________________</td>
				</tr>
				
			</table>
					
		</td>
	</tr>
	<tr>
		<td>
			<table width="100%"  cellspacing="0" cellpadding="0">
				<TR>
					<TD COLSPAN="2" HEIGHT="100"></TD>
				</TR>
				<tr>
					<td width="50%"> _______________________ </td>
					<td width="50%"> _______________________ </td>
				</tr>
				<tr>
					<td> <b>ISSUED BY</b> </td>
					<td> <B>APPROVED BY </B> </td>
				</tr>
			</table>			
		</td>
	</tr>
	
	
</table>	
</body>
</html>
