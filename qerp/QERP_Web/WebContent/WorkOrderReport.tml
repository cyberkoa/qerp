<html 
xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">

 
<body>
<table  border="1" cellspacing="0" cellpadding="0">
	<tr>
		<td align="center">
			<h1><b>${CompDesc}</b></h1>			
		</td>
	</tr>
	<tr>
		<td align="center">
			<b>ORDER FORM</b>			
		</td>
	</tr>
	<tr>
		<td width="800">
			<table width="100%" border="1" cellspacing="0" cellpadding="0">
				<tr>
					<td>
						<b>Order No:</b>
					</td>
					<td>
						 ${printOrderNo}
					</td>
					<td>
						<b>Order Date: </b>
					</td>
					<td>
						 ${printOrderDate}
					</td>
				</tr>
				
				<tr>
					<td>
						<b>P/O No : </b>
					</td>
					<td>
						 ${printPOno}
					</td>
					<td>
						<b>Delivery Date:</b>
					</td>
					<td>
						 ${printDeliveryDate}
					</td>
				</tr>
				
				<tr>
					<td>
						<b>Customer No:</b>
					</td>
					<td>
						 ${printCustomerNo}
					</td>
					<td colspan="2">
						
					</td>
				</tr>
				
				<tr>
					<td>
						<b>Brand Code:</b>
					</td>
					<td>
						  Blue berry
					</td>
					<td colspan="2">
						 
					</td>
				</tr>
				<tr>
					<td colspan="4"></td>
				</tr>
			</table>	
		</td>
	</tr>
	<tr>
		<td>
			<b>Raw Material:-</b>			
		</td>
	</tr>
	
	<tr>
		<td>
			<table width="100%" border="1" cellspacing="0" cellpadding="0">
				<tr>
					<th width="100"> Code No </th>
					<th width="100"> Film Type </th>
					<th width="100"> Thickness </th>
					<th width="100"> Width </th>
					<th width="100"> Length </th>
					<th width="100"> Quantity </th>
					<th width="100"> Process </th>
					<th width="150"> Production Date </th>					
				</tr>
				<t:loop source="RawMats" value="RawMat">
					<tr>
						<td>${CodeNo}</td>
						<td>${Type}</td>
						<td>${Thickness}</td>
						<td>${Width}</td>
						<td>${Length}</td>
						<td>${Quantity}</td>
						<td>${Process}</td>
						<td>${ProductionDate}</td>
						
						
					</tr>
				</t:loop>

			</table>			
		</td>
	</tr>
	
	
</table>	
</body>
</html>
